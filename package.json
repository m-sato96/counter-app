    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "nuxt": "^3.8.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.24"
  }
